{
  "name": "ncs-mobile-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "-android": "react-native run-android",
    "-ios": "react-native run-ios",
    "android": "react-native run-android --variant=developdebug",
    "ios": "react-native run-ios --scheme Develop --configuration Debug --simulator='iPhone 14'",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "android:dev:d": "react-native run-android --variant=developdebug",
    "android:dev:r": "react-native run-android --variant=developrelease",
    "android:stg:d": "react-native run-android --variant=stagedebug",
    "android:stg:r": "react-native run-android --variant=stagerelease",
    "android:prd:d": "react-native run-android --variant=productdebug",
    "android:prd:r": "react-native run-android --variant=productrelease",
    "ios:dev:d": "react-native run-ios --scheme Develop --configuration Debug --simulator='iPhone 14'",
    "ios:dev:r": "react-native run-ios --scheme Develop --configuration Release --device 'TEST iPhone'",
    "ios:stg:d": "react-native run-ios --scheme Stage --configuration Debug --simulator='iPhone 14'",
    "ios:stg:r": "react-native run-ios --scheme Stage --configuration Release --simulator='iPhone 14'",
    "ios:prd:d": "react-native run-ios --scheme Product --configuration Debug --simulator='iPhone 14'",
    "ios:prd:r": "react-native run-ios --scheme Product --configuration Release --device 'TEST iPhone'"
  },
  "dependencies": {
    "@baronha/react-native-multiple-image-picker": "^0.4.13",
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.1",
    "axios": "^1.2.0",
    "react": "18.1.0",
    "react-native": "0.70.6",
    "react-native-config": "^1.4.11",
    "react-native-dropdown-select-list": "^2.0.4",
    "react-native-image-picker": "^4.10.2",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-paper": "^5.0.0-rc.10",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-safearea-height": "^1.0.5",
    "react-native-screens": "^3.18.2",
    "react-native-svg": "^13.6.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-toast-message": "^2.1.5",
    "react-native-vector-icons": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "react-test-renderer": "18.1.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
